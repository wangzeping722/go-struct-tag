{
	"name": "go-struct-tag",
	"displayName": "Go struct tag",
	"description": "fork自go-struct-tag，增加小驼峰功能, 使用小驼峰格式需要增加配置 goStructTag.tagType: lowerCamelCase",
	"keywords": [
		"go",
		"golang",
		"struct",
		"tag",
		"complete",
		"json"
	],
	"version": "0.0.12",
	"publisher": "wangzeping",
	"repository": {
		"type": "git",
		"url": "https://github.com/wangzeping722/go-struct-tag"
	},
	"license": "MIT",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Programming Languages",
		"Extension Packs"
	],
	"activationEvents": [
		"onLanguage:go"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration":[
			{
			  "title": "go-struct-tag",
			  "properties": {
				"goStructTag.tagType": {
				  "type": "string",
				  "default": "default",
				  "description": "tag type"
				}
			  }
			}
		  ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
